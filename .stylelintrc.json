{
	"extends": "stylelint-config-standard-scss",
	"ignoreFiles": [
		"dist/*"
	],
	"rules": {
		"selector-class-pattern": "^(?:(?:o|c|u|t|s|is|has|_|js|qa)-)?[a-zA-Z0-9]+(?:-[a-zA-Z0-9]+)*(?:__[a-zA-Z0-9]+(?:-[a-zA-Z0-9]+)*)?(?:_[a-zA-Z0-9]+(?:-[a-zA-Z0-9]+)*)?(?:\\[.+\\])?$",
		"property-no-vendor-prefix": [null, ""]
	}
}
